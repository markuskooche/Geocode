{
    "name": "markuskooche/geocode",
    "description": "A simple wrapper for (reverse) geocoding.",
    "keywords": [
        "php", "geocode", "reverse", "geocoding", "latitude", "longitude", "api"
    ],
    "homepage": "https://github.com/markuskooche/geocode",
    "license": "MIT",
    "authors": [
        {
            "name": "Markus Koch",
            "email": "markus.koch@ptyspark.com",
            "homepage": "https://markuskooche.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/markuskooche/geocode/issues"
    },
    "require": {
        "php": "^8.0",
        "illuminate/collections": "^9.0",
        "illuminate/http": "^9.0",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "nunomaduro/larastan": "^2.1.4",
        "orchestra/testbench": "^7.4",
        "laravel/lumen-framework": "^9.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Markuskooche\\Geocode\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Markuskooche\\Geocode\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Markuskooche\\Geocode\\GeocodeServiceProvider"
            ]
        }
    }
}
